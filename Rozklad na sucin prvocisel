prvocisla = []
for i in range(1000):
    pole = []
    for p in range(1000):
        if i % (p+1) == 0:
            pole.append('1')
    if len(pole) == 2:
        prvocisla.append(i)
a =  input('Zadaj číslo')
a = int(a)
k = 0
f = 0
for i in prvocisla:
    for g in range(50):
        if a % i == 0:
            k += 1
            a = a/i
    if k > 0:
        print( i, 'na', k)
        k = 0
